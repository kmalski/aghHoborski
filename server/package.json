{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for the application for the \"Awantura o Nauke\" science quiz",
  "author": "Konrad Malski",
  "private": true,
  "main": "src/server.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "rimraf build/ && tsc",
    "prestart": "npm run build",
    "start": "node build/src/server.js",
    "server": "tsnd --rs --respawn src/server.ts",
    "test": "mocha -r ts-node/register 'test/**/*.ts'",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@typegoose/typegoose": "^8.1.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.7",
    "socket.io": "^4.1.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.21",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.2",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.2.0",
    "eslint": "^7.32.0",
    "mocha": "^9.0.3",
    "mongodb-memory-server-core": "^7.3.6",
    "rimraf": "^3.0.2",
    "socket.io-client": "^4.1.3",
    "ts-node": "^10.2.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3",
    "utf-8-validate": "^5.0.5"
  },
  "config": {
    "mongodbMemoryServer": {
      "downloadDir": "mongod"
    }
  }
}
