{
  "name": "server",
  "version": "0.1.0",
  "description": "Server for the application for the \"Awantura o Nauke\" science quiz",
  "author": "Konrad Malski",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "build": "rimraf build/ && tsc",
    "prestart": "npm run build",
    "start": "node build/server.js",
